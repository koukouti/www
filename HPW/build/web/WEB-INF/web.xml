<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>in.co.sneh.controller.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bmi</servlet-name>
        <servlet-class>com.example.model.Bmi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyDatabase</servlet-name>
        <servlet-class>com.mysql.database.MyDatabase</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Bmi</servlet-name>
        <url-pattern>/Bmi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/FileUploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyDatabase</servlet-name>
        <url-pattern>/MyDatabase</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
